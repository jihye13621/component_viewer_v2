[
	"ds-cnbc-lite-dynamic",
	[
		["dsx-mixins", ["captions"]],
		["ds-cnbc-lite-mixins", "color-1", "color-2"],
		["dsx-mixins", ["boilerplate"]],
		{
			"key": "position",
			"display": "Position",
			"type": "position",
			"default": 5,
			"required": false
		}
	],
	[
		["dsx-background", {
			"enabled": ["$", ["exists", ["var", "background"]]],
			"color": ["$", ["var", "background"]]
		}],
		["null", {
			"mixins": ["dsx-mixins", ["set-dsx-su", "safezone"]]
		}, [
			["null", {
				"max-width": "800nsu",
				"layout": "vertical packed",
				"align": ["$", ["var", "#align"]]
			}, [
				["text", {
					"mixins": ["ds-cnbc-lite-mixins", ["text-1"]],
					"justify": ["$", ["var", "#align"]],
					"transcript": ["$",
						["transcript-split",
							["var", "transcript"],
							["max", 1, ["floor", ["/", 10, ["var", "size"]]]]
						]
					],
					"color": ["$", ["var", "color-1"]],
					"stroke": "12su",
					"stroke-color": ["$", ["var", "color-2"]],
					"animations": [
						{
							"opacity": 0,
							"scale": 0
						},
						{
							"mix": [[0, 0], [0.3, 1]],
							"opacity": 1,
							"scale": 1,
							"easing": "easeOutBack"
						}
					]
				}]
			]]
		]]
	]
]