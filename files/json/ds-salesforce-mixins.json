{
	"params": {
		"font-headline": {
			"key": "font-headline",
			"display": "Font Headline",
			"type": "string",
			"default": "./media/SalesforceSans-Bold.ttf",
			"required": false,
			"private": true
		},
		"font-body": {
			"key": "font-body",
			"display": "Font Body",
			"type": "string",
			"default": "./media/SalesforceSans-Regular.ttf",
			"required": false,
			"private": true
		}
	},
	"text": {
		"measure": "letter-size"
	},
	"text-display": {
		"font": ["$", ["var", "font-headline"]],
		"height": "200su",
		"leading": "10su"
	},
	"text-headline": {
		"font": ["$", ["var", "font-headline"]],
		"height": "80su",
		"leading": "15su"
	},
	"text-title": {
		"font": ["$", ["var", "font-body"]],
		"height": "90su",
		"leading": "20su"
	},
	"text-body": {
		"font": ["$", ["var", "font-body"]],
		"height": "55su",
		"leading": "5su"
	},
	"text-label": {
		"font": ["$", ["var", "font-body"]],
		"height": "40su",
		"leading": "10su"
	}
}