[
	"ds-gofundme-text",
	[
		["dsx-mixins",
			"text"
		],
		["ds-gofundme-mixins",
			"font-bold"
		]
	],
	[
		["null", {
			"mixins": ["dsx-mixins",
				"set-su",
				"safezone"
			],
			"layout": "vertical packed",
			"align": "bottom",
			"min-width": "100rw"
		}, [
			["null", {
				"flex": {
					"position": "fixed"
				},
				"padding": [
					"-100su",
					0,
					0,
					0
				],
				"opacity": ["animate",
					[0, 0],
					["+500", 1],
					["100%", 0],
					["-500", 1]
				]
			}, [
