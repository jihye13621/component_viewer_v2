				}]
			]],
			["text", {
				"font": ["$", ["var", "font-bold"]],
				"measure": "letter-size",
				"height": "60su",
				"leading": 0.2,
				"justify": "bottom left",
				"string": ["$", ["var", "text"]],
				"color": [1, 1, 1, 1],
				"animations": [
					{
						"translate": [
							0,
							"100su"
						]
