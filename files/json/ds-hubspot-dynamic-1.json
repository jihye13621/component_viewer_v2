[
	"ds-hubspot-dynamic-1",
	[
		["dsx-mixins",
			"captions"
		],
		{
			"key": "color-1",
			"display": "Color 1",
			"type": "color",
			"default": "#FEF4EA",
			"required": false
		},
		{
			"key": "color-2",
			"display": "Color 2",
			"type": "color",
			"default": "#192733",
			"required": false
		},
		["dsx-mixins",
			"boilerplate-captions"
		],
		{
			"key": "max-words",
			"display": "Words",
			"type": "number",
			"default": 4,
			"required": false,
			"ui": {
				"input": "slider",
				"display": "number",
				"step": 1,
				"minimum": 1,
				"maximum": 12
			}
		},
		{
			"key": "position",
			"display": "Position",
			"type": "position",
			"default": [5, 0, 0],
			"ui": {
				"grid": true,
				"offset": true,
				"minimum": -100,
				"maximum": 100
			},
			"required": false
		}
	],
	[
		["null", {
			"mixins": ["dsx-mixins", 
				"set-dsx-su",
				"safezone",
				"offset"
			]
		}, [
			["null", {
				"max-width": "800nsu",
				"layout": "vertical packed",
				"align": ["$", ["var", "#align"]]
			}, [
				["text", {
					"mixins": ["ds-hubspot-mixins",
						"font-text-large"
					],
					"measure": "letter-size",
					"leading": "10su",
					"justify": ["$", ["var", "#align"]],
					"transcript": ["$",
						["transcript-split",
							["var", "transcript"],
							["var", "max-words"]
						]
					],
					"color": ["$", ["var", "color-1"]],
					"stroke": "13su",
					"stroke-color": ["$", ["var", "color-2"]],
					"animations": [
						{
							"opacity": 0,
							"scale": 0
						},
						{
							"mix": [[0, 0], [0.8, 1]],
							"opacity": 1,
							"scale": 1,
							"easing": "easeOutBack"
						}
					]
				}]
			]]
		]]
	]
]